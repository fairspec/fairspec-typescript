{
  "name": "@fairspec/terminal",
  "type": "module",
  "version": "0.0.0-dev",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "bin": {
    "fairspec": "./build/entrypoints/run.js"
  },
  "license": "MIT",
  "author": "Evgeny Karev",
  "repository": "https://github.com/fairspec/fairspec-typescript",
  "description": "Fairspec TypeScript is a fast data management framework built on top of the Fairspec standard and Polars DataFrames",
  "keywords": [
    "fairspec",
    "data",
    "polars",
    "dataframe",
    "jsonschema",
    "tableschema",
    "typescript",
    "validation",
    "quality",
    "fair",
    "terminal",
    "cli"
  ],
  "scripts": {
    "build": "tsc && pnpm build:copy && pnpm build:mode",
    "build:copy": "cp entrypoints/*.cmd build/entrypoints && cp package.json build",
    "build:mode": "chmod +x ./build/entrypoints/*.js",
    "type": "tsc --noEmit"
  },
  "dependencies": {
    "@bomb.sh/tab": "^0.0.11",
    "@commander-js/extra-typings": "^14.0.0",
    "@fairspec/library": "workspace:*",
    "commander": "^14.0.0",
    "json-colorizer": "^3.0.1",
    "picocolors": "^1.1.1",
    "tasuku": "^2.0.5"
  },
  "devDependencies": {
    "@types/prettyjson": "^0.0.33"
  }
}
